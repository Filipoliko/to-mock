{
	"name": "to-mock",
	"version": "1.2.0",
	"description": "The utility for creating mocked classes and objects which you can use for your unit tests",
	"main": "toMock.js",
	"scripts": {
		"lint": "node_modules/.bin/eslint -c ./.eslintrc.js --fix './**/*.js' --ignore-path ./.prettierignore",
		"test": "node_modules/.bin/ava",
		"preversion": "npm test && npm run lint",
		"postversion": "git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish",
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mjancarik/to-mock.git"
	},
	"keywords": [
		"mock",
		"unit",
		"tests",
		"integration",
		"test",
		"javascript",
		"ES6",
		"class",
		"Object"
	],
	"publishConfig": {
		"registry": "http://registry.npmjs.org/"
	},
	"author": "Miroslav Jancarik",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/mjancarik/to-mock/issues"
	},
	"homepage": "https://github.com/mjancarik/to-mock#readme",
	"devDependencies": {
		"ava": "^0.22.0",
		"babel-eslint": "^8.0.2",
		"babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
		"coveralls": "^3.0.0",
		"eslint": "^4.10.0",
		"eslint-config-last": "^0.0.3",
		"eslint-config-prettier": "^2.7.0",
		"eslint-plugin-prettier": "^2.3.1",
		"prettier": "^1.8.2",
		"nyc": "^11.2.1",
		"sinon": "^4.0.0"
	},
	"ava": {
		"babel": {
			"plugins": [
				"transform-es2015-modules-commonjs"
			]
		}
	}
}
